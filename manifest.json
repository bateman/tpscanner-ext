{
    "manifest_version": 3,
    "name": "TPscanner",
    "description": "__MSG_extensionDescription__",
    "version": "0.6.1",
    "default_locale": "en",
    "icons": {
        "48": "images/icon.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://www.trovaprezzi.it/*",
        "https://trovaprezzi.it/*"
    ],
    "action": {
        "default_icon": "images/icon.png",
        "default_popup": "html/popup.html"
    }
}